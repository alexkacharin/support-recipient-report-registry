{
    "name": "kurganobl/registry-constructor",
    "description": "Yii 2 Advanced Project Template",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3",
        "npm-asset/jquery": "^3.6",
        "ext-json": "*",
        "ext-curl": "*",
        "phpoffice/phpspreadsheet": "^1.12",
        "kartik-v/yii2-widget-select2": "dev-master",
        "kartik-v/yii2-widget-datepicker": "^1.4",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-bootstrap4": "~2.0.6",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-authclient": "~2.2.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "2amigos/yii2-tinymce-widget": "~1.1",
        "kartik-v/yii2-widget-datetimepicker": "^1.4",
        "kartik-v/yii2-widget-fileinput": "^1.1",
        "npm-asset/store": "^2.0",
        "npm-asset/bootstrap-table": "^1.18.3",
        "nesbot/carbon": "^2.53",
        "ext-intl": "*",
        "unclead/yii2-multiple-input": "~2.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.1.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.0",
        "codeception/module-yii2": "^1.0",
        "codeception/module-filesystem": "^1.0",
        "codeception/verify": "~0.5.0 || ~1.1.0",
        "symfony/browser-kit": ">=2.7 <=4.2.4"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "autoload": {
        "psr-4": {
            "dektrium\\rbac\\": "common/modules/yii2-rbac",
            "dektrium\\user\\": "common/modules/yii2-user",
            "Matodor\\RegistryConstructor\\": "common/modules/matodor/registry-constructor",
            "Matodor\\Common\\": "common/modules/matodor/common"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
